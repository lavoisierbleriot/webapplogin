pipeline{
         agent any
    stages {
        stage('download the code from github') {
           steps {
              git branch: 'Ekoume', 
              url: 'https://github.com/lavoisierbleriot/webapplogin.git'
    }
    }
    stage('Build the code to an artifact ') {
           steps {
             sh 'mvn package'
    }
    }
   // stage('Deploy artifact to nexus') {
        //   steps {
      //sh 'mvn package'
    //}
 // }

}



}

